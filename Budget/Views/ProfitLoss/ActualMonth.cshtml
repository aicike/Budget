@section scripts{
    <script type="text/javascript">
    
        //获取数值
        function getValue(obj) {
            var value = 0;
            if ($(obj).text().length == 0) {
                value = 0;
            } else {
                if (isNaN($(obj).val())) {
                    $(obj).text("0");
                }
                value = parseFloat($(obj).text());
            }
            return value;
        }
        $(function () {
            $(".itable .jstr").each(function (i, n) {
                var week1 = getValue($(n).find(".zslabel[row='1']"));
                var week2 = getValue($(n).find(".zslabel[row='2']"));
                var week3 = getValue($(n).find(".zslabel[row='3']"));
                var week4 = getValue($(n).find(".zslabel[row='4']"));
                var week = week1 + week2 + week3 + week4;
                var ysLabel = $(n).find(".yslabel");
                var yeLabel = $(n).find(".yelabel");
                var balance = parseFloat((week - ysLabel.text()).toFixed(13));
                if (balance < 0) {
                    yeLabel.text(balance).addClass("icolor_red").removeClass("icolor_green");
                } else {
                    yeLabel.text(balance).addClass("icolor_green").removeClass("icolor_red");
                }
            });
        });

    </script>
}
