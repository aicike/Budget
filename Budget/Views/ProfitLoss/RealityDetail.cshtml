@section scripts{
    <script type="text/javascript">
        //获取数值
        function getValue(obj) {
            var value = 0;
            if ($(obj).val().length == 0) {
                value = 0;
            } else {
                if (isNaN($(obj).val())) {
                    $(obj).val("0");
                }
                value = parseFloat($(obj).val());
            }
            return value;
        }
        $(".itable :text").keyup(function () {
            var week1 = getValue($(this).parents("tr").find(":text[row='1']"));
            var week2 = getValue($(this).parents("tr").find(":text[row='2']"));
            var week3 = getValue($(this).parents("tr").find(":text[row='3']"));
            var week4 = getValue($(this).parents("tr").find(":text[row='4']"));
            var week = week1 + week2 + week3 + week4;
            var ysLabel = $(this).parents("tr").find(".yslabel");
            var yeLabel = $(this).parents("tr").find(".yelabel");
            var balance = week - parseFloat(ysLabel.text());
            yeLabel.text(balance);
        });
    </script>
}
